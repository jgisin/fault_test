<% @page_title = "Details" %>
    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <p class="navbar-text">Currently logged in as: <b><%= session[:user_name].upcase %></b></p>
        </div>
                <div id="navbar" class="navbar-collapse collapse">
                  <form class="navbar-form navbar-right">
                    <div class="form-group">
            <%= form_tag(:controller => 'access', :action => 'attempt_login') do %>
            
              <%= text_field_tag 'user_name', nil, class: "form-control", placeholder: "Username" %>
            </div>
            <div class="form-group">
              <%= password_field_tag 'password', nil, class: "form-control", placeholder: "Password" %>
            </div>
            <%= submit_tag "Log In", class: "btn btn-success" %>
              <% end %>
          </form>
        </div><!--/.navbar-collapse -->
      </div>
    </nav>
<div class="jumbotron">
  <div class="container">
    <h1>Panel Details</h1>
  </div>
</div>
<p class="notice"><%= notice %></p>
<div class="container">
<p>
  <strong>Panel Name:</strong>
  <%= @panel.panel_name %>
</p>

<p>
  <strong>Wire length:</strong>
  <%= @panel.wire_length %>
</p>

<p>
  <strong>Init fault:</strong>
  <%= @panel.init_fault %>
</p>

<p>
  <strong>Runs:</strong>
  <%= @panel.runs %>
</p>

<p>
  <strong>Voltage:</strong>
  <%= @panel.voltage %>
</p>

<p>
  <strong>C value:</strong>
  <%= @panel.c_value %>
</p>

<p>
  <strong>Project ID:</strong>
  <%= @panel.project_id %>
</p>
<%= link_to 'Edit', edit_panel_path(@panel), class: "btn btn-md btn-success" %>    
<%= link_to "Back to Panels", {controller: "panels", action: "index",
 project_id: @panel.project_id}, class: "btn btn-md btn-default" %>
 </div>